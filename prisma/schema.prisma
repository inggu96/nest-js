

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DB_URL")
}

model Book {
  id        Int      @id @default(autoincrement())
  author    String
  name      String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  deletedAt DateTime?
}

model Phone {
    id Int @id  @default(autoincrement())
    number String
    type String
}

model User{ 
  id Int @id  @default(autoincrement())
  name      String?
  email     String?   @unique
  password  String?
  birth     String?
  nickname  String?
}

enum UserType{
  ADMIN
  USER
}

enum ReportType{
  PENDING
  IGNORE
  USER_DELETE
}

enum UserGender{
  MALE
  FEMALE
}